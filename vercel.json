{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/MD/(.*)",
      "dest": "/MD/$1"
    },
    {
      "src": "/files/(.*)",
      "dest": "/files/$1"
    }
  ]
}
